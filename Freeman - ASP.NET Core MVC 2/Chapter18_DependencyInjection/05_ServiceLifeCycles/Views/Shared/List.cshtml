@{ Layout = "_Layout"; }

<div>
    <table>

        <thead>
        <tr>
            <th>Key</th><th>Value</th>
        </tr>
        </thead>

        <tbody>
        @foreach (var kvp in ViewData)
        {
            <tr>
                <td>@kvp.Key</td><td>@kvp.Value</td>
            </tr>
        }
        </tbody>

    </table>
</div>
